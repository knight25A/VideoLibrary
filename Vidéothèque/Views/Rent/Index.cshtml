@model Vidéothèque.ViewModels.AllRentsViewModel


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
@foreach (var rent in Model.Rents)
{
    @rent.Invoice.User.Email
    <br />
    @rent.Invoice.User.Name
    <br />
    @rent.Invoice.Movie.Title
    <br />
    @rent.Invoice.Movie.MovieGenre.Name
}